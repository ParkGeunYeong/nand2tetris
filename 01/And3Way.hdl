/**
 * 3-Way And gate: 
 * out = (a and b and c)
 */

CHIP And3Way {
    IN a, b, c;
    OUT out;

    PARTS:
    And(a = a, b = b, out = a1);
    And(a = a1, b = c, out = out);
}
