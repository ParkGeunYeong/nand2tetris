/**
 * 4 And gate: 
 * out = (in[0] and in[1] and in[2] and in[3])
 */

CHIP And4 {
    IN in[4];
    OUT out;

    PARTS:
    And(a = in[0], b = in[1], out = o1);
    And(a = in[2], b = o1, out = o2);
    And(a = in[3], b = o2, out = out);
}
